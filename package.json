{
  "name": "generator-ruby-cmd",
  "version": "1.0.3",
  "description": "Simple command line Ruby program, bundled as a gem with Rrspec and Cucumber/Aruba support",
  "homepage": "https://github.com/snada/generator-ruby-cmd",
  "author": {
    "name": "snada",
    "email": "stefano.nada@gmail.com",
    "url": "https://snada.it"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "ruby",
    "cmd",
    "gem",
    "rspec",
    "cucumber",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^4.2.1",
    "npm-audit": "0.0.3",
    "prettier": "^1.16.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "case": "^1.6.1",
    "chalk": "^2.4.2",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "npm-audit",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "snada/generator-ruby-cmd",
  "license": "MIT"
}
